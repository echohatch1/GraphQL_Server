# Create a new product
mutation {
  createProduct(
    data: {
      price: 42
      desc: "Here is some product"
      name: "Some product"
    }
  ) {
    id
    name
    price
    desc
  }
}

# get all products
query {
  products {
    id
    name
  }
}

# Update a product
mutation {
  updateProduct(
    data: {
      price: 9
    }
    where: {
      id: "cjtrxz0v201su0717e4uzhopq"
    }
  ) {
    id
    name
    price
  }
}